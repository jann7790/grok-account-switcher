{
    "manifest_version": 3,
    "name": "grok Account Switcher",
    "version": "1.0",
    "description": "Quickly switch between multiple grok accounts.",
    "permissions": [
      "cookies",
      "storage",
      "tabs",
      "scripting"
    ],
    "host_permissions": [
      "https://grok.com/*"
    ],
    "action": {
      "default_popup": "popup.html"
    },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  }
  }
